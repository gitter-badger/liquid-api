{
  "extends": "airbnb-base/legacy",
  "parserOptions": {
    "ecmaVersion": 6,
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "consistent-return": "off",
    "func-names": "off",
    "global-require": "off",
    "max-len": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-prototype-builtins": "off",
    "padded-blocks": "off",
    "semi": ["error", "never"],
    "vars-on-top": "off",
  },
}
